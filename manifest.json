{
    "manifest_version": 3,
    "name": "Resource Override Custom",
    "version": "1.0",
    "permissions": [
      "storage",
      "declarativeNetRequest"
    ],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    }
  }
  